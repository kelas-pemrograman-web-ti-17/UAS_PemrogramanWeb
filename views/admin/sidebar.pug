// Left side column. contains the logo and sidebar
aside.main-sidebar
    // sidebar: style can be found in sidebar.less
    section.sidebar
        // Sidebar user panel
        .user-panel
            .pull-left.image
                img.img-circle(src='adminlte/dist/img/user2-160x160.jpg' alt='User Image')
            .pull-left.info
                p Alexander Pierce
                a(href='#')
                    i.fa.fa-circle.text-success
                    |  Online
        // search form
        form.sidebar-form(action='#' method='get')
            .input-group
                input.form-control(type='text' name='q' placeholder='Search...')
                span.input-group-btn
                    button#search-btn.btn.btn-flat(type='submit' name='search')
                        i.fa.fa-search
        // /.search form
        // sidebar menu: : style can be found in sidebar.less
        ul.sidebar-menu(data-widget='tree')
            li.header MAIN NAVIGATION
            li.active.treeview
                a(href='#')
                    i.fa.fa-dashboard
                    span Dashboard
            li.treeview
                a(href='#')
                    i.fa.fa-laptop
                    span Data Mahasiswa
                    span.pull-right-container
                        i.fa.fa-angle-left.pull-right
                ul.treeview-menu
                    li
                        a(href='pages/UI/general.html')
                            i.fa.fa-circle-o
                            |  Input Nama
                    li
                        a(href='pages/UI/icons.html')
                            i.fa.fa-circle-o
                            |  Input Npm
                    li
                        a(href='pages/UI/buttons.html')
                            i.fa.fa-circle-o
                            |  Data Mahasiswa
                    li
                        a(href='pages/UI/sliders.html')
                            i.fa.fa-circle-o
                            |  Data Fakultas
                    li
                        a(href='pages/UI/timeline.html')
                            i.fa.fa-circle-o
                            |  Data Prog. Studi
                    li
                        a(href='pages/UI/modals.html')
                            i.fa.fa-circle-o
                            |  Data Dosen
